Dashboard dynamic_data_source_hv {
  title: 'Dynamic Data Source Hv'
  description: ''''''

  view: CanvasLayout {
    label: 'View 1'
    height: 840
    grid_size: 20
    block v1 {
      position: pos(20, 20, 460, 600)
      layer: 1
    }
  }

  theme: H.themes.classic
  block v1: VizBlock {
    label: 'cities'
    viz: DataTable {
      dataset: dynamic_data_source_data_set
      fields: [
        VizFieldFull {
          ref: ref('ecommerce_cities_demo', 'id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('ecommerce_cities_demo', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('ecommerce_cities_demo', 'country_code')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
}