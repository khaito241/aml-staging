Dashboard one_n_filter_mapping {
  title: '1-N Filter Mapping use-case'
  description: ''''''

  view: CanvasLayout {
    label: 'View 1'
    width: 1100
    height: 780
    grid_size: 20
    block v1 {
      position: pos(60, 120, 1020, 560)
      layer: 1
    }
    block f1 {
      position: pos(60, 0, 300, 100)
      layer: 2
    }
  }

  theme: H.themes.classic
  block v1: VizBlock {
    label: 'Buyers Sellers pair transaction'
    viz: DataTable {
      dataset: ecom_transactions
      filter {
        field: ref('transactions_buyers_sellers', 'transaction_date')
        operator: 'matches'
        value: '01 Aug 2023'
      }
      filter {
        field: ref('sellers', 'seller_name')
        operator: 'not_null'
        value: []
      }
      fields: [
        VizFieldFull {
          ref: ref('transactions_buyers_sellers', 'transaction_date')
          transformation: 'datetrunc day'
          format {
            type: 'date'
            pattern: 'LLL dd, yyyy'
          }
        },
        VizFieldFull {
          ref: ref('buyers', 'buyer_name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('buyers', 'buyer_gender')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('sellers', 'seller_name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('sellers', 'seller_gender')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('transactions_buyers_sellers', 'quantity')
          aggregation: 'sum'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block f1: FilterBlock {
    label: 'Gender'
    type: 'field'
    source: FieldFilterSource {
      dataset: ecom_transactions
      field: ref('buyers', 'buyer_gender')
    }
    default {
      operator: 'is'
      value: 'female'
    }
    settings {
      input_type: 'single'
    }
  }
  interactions: [
    FilterInteraction {
      from: 'f1'
      to: [
        CustomMapping {
          block: 'v1'
          field: ref('param_model', 'gender_param')
        }
      ]
    }
  ]
}