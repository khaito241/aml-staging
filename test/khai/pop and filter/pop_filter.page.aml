Dashboard pop_filter {
  title: 'Pop Filter'
  description: ''''''

  view: CanvasLayout {
    label: 'View 1'
    grid_size: 20
    mobile {
      mode: 'auto'
    }
    block v1 {
      position: pos(20, 100, 1160, 540)
      layer: 1
    }
    block v2 {
      position: pos(20, 740, 1160, 540)
      layer: 1
    }
    block t1 {
      position: pos(20, 40, 560, 60)
      layer: 2
    }
    block t2 {
      position: pos(20, 680, 560, 60)
      layer: 1
    }
    width: 1240
    height: 1320
  }

  theme: H.themes.classic
  block v1: VizBlock {
    label: 'Revenue by Month Created At'
    viz: LineChart {
      dataset: raw_aql_ecommerce
      filter {
        field: ref('orders', 'created_at')
        operator: 'matches'
        value: '2022-08 to 2023-10'
      }
      x_axis: VizFieldFull {
        ref: ref('orders', 'created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        settings {
          show_data_label_by: 'value'
        }
        series {
          field: VizFieldFull {
            ref: 'revenue'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 10000
        x_axis_show_null_datetime: false
        pop {
          field: ref('orders', 'created_at')
          duration: 1
          granularity: 'year'
          show_growth_rate: true
        }
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'Revenue by Month Created At (dup)'
    viz: LineChart {
      dataset: raw_aql_ecommerce
      calculation filtered_revenue {
        label: 'Filtered Revenue'
        formula: @aql revenue 
  | where(
      orders.created_at >= @2022-08
      and orders.created_at <= @2023-10
    );;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation revenue_last_year {
        label: 'Revenue Last Year'
        formula: @aql filtered_revenue 
  | relative_period(orders.created_at, interval(-1 year));;
        calc_type: 'measure'
        data_type: 'number'
      }
      x_axis: VizFieldFull {
        ref: ref('orders', 'created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
          pattern: 'LLL yyyy'
        }
      }
      y_axis {
        settings {
          show_data_label_by: 'value'
        }
        series {
          field: VizFieldFull {
            ref: 'filtered_revenue'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
        series {
          field: VizFieldFull {
            ref: 'revenue_last_year'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 10000
        x_axis_show_null_datetime: false
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block t1: TextBlock {
    content: @md This Widget uses Viz PoP and Viz Condition;;
  }
  block t2: TextBlock {
    content: @md This Widget uses AQL PoP and Condition within the AQL metric ;;
  }
}