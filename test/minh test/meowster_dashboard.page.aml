Dashboard meowster_dashboard {
  title: 'Meowster Dashboard'
  description: '''eiuuuuuwwwww'''
  block v1: VizBlock {
    label: 'GMV by age group'
    viz: PieChart {
      dataset: ecommerce
      filter {
        field: ref('users', 'age_group')
        operator: 'is'
        value: [
          'Under 18 .',
          '18 - 22'
        ]
      }
      legend: VizFieldFull {
        ref: ref('users', 'age_group')
        format {
          type: 'text'
        }
      }
      series {
        field {
          ref: ref('order_derived', 'gross_merchandise_value')
          aggregation: 'custom'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'Cumulative Buyers Count'
    viz: ColumnChart {
      dataset: ecommerce
      x_axis: VizFieldFull {
        ref: ref('merchants', 'created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      legend: VizFieldFull {
        ref: ref('users', 'gender')
        format {
          type: 'text'
        }
      }
      y_axis {
        series {
          field {
            label: 'Customers count'
            ref: ref('users', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
  // }
          }
        }
      }
    }
  }
  block v3: VizBlock {
    label: 'Conversion Funnel'
    viz: ConversionFunnel {
      dataset: ecommerce
      calculation f_9165bde {
        label: 'Refunded Orders'
        formula: @aml count(case(
  when: order_derived.order_status == 'refunded'
  , then: order_derived.order_id
  , else: null
));;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation f_9b4b169 {
        label: 'Cancelled Orders'
        formula: @aml count(case(
  when: order_derived.order_status == 'cancelled'
  , then: order_derived.order_id
  , else: null
));;
        calc_type: 'measure'
        data_type: 'number'
      }
      calculation f_6e32a9b {
        label: 'Delivered Orders'
        formula: @aml count(case(
  when: order_derived.order_status == 'delivered'
  , then: order_derived.order_id
  , else: null
));;
        calc_type: 'measure'
        data_type: 'number'
      }
      breakdown_by: VizFieldFull {
        ref: ref('locations_1', 'country_name')
        format {
          type: 'text'
        }
      }
      values: [
        VizFieldFull {
          ref: 'f_6e32a9b'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: 'f_9b4b169'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: 'f_9165bde'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
    }
  }
  block t1: TextBlock {
    content: @md   <h1 style="color:#FFFFFF; font-weight: bolder;font-size:56px; line-height: 3.5rem">Meowster Dashboard</h1>
<h3 style="color:#80ABEB; font-size:30px">Ecommerce report</h3>;;
