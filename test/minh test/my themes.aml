PageTheme orange {
  custom_css: @css
    .dac-canvas-layout {
    background: radial-gradient(circle, #ffd191 10%, transparent 11%),radial-gradient(circle at bottom left, #ffd191 5%, transparent 6%),radial-gradient(circle at bottom right, #ffd191 5%, transparent 6%),radial-gradient(circle at top left, #ffd191 5%, transparent 6%),radial-gradient(circle at top right, #ffd191 5%, transparent 6%);
        background-size: 1em 1em;
        background-color: #ffffff;
        opacity: 1
    }
   ;;
  
  canvas {
    background {
      bg_image: 'linear-gradient(to top, #F2C58C, #FFFAF3)'
    }
    border {
      border_width: 1
      border_radius: 2
      border_color: '#F3DFC4'
    }
    shadow: 'none'
  }
  block {
    
  }
  viz {
    // Table Theme
    table {
      general {
      bg_color: '#FFFAF3';
        banding_color: '#FFEFD8';
        hover_color: '#F2B455'; 
        font_size: '12px'; 
        font_color: '#7B5604'; 
        border_color: '#CE780C'; 
        grid_color: '#CE780C'; 
      }

      // Header Styles
      header {
        bg_color: '#E5951A';
        font_color: '#513519'
        font_size: '14px'; 
        font_weight: 'bold'; 
      }

      // Pivot table attributes
      row_header {
        bg_color: '#FFC572';
        font_size: '12px'; 
        font_weight: 'bold'; 
      }

      column_header {
        bg_color: '#E5951A';
        font_color: '#513519'
        font_size: '12px'; 
        font_weight: 'bold'; 
      }

      // Metric Sheet attributes
      sub_title {
        font_size: '12px';
        font_weight: 'light';
      }
    }
  }
}


PageTheme sakura {
  custom_css: @css
      @import url('https://fonts.googleapis.com/css2?family=Playwrite+GB+S:ital,wght@0,100..400;1,100..400&display=swap');
  ;;
  background {
      bg_image: 'linear-gradient(to top, #FFF5FB, #FAE0EA)'
    }
    canvas {
      border {
        border_width: 2
        border_color: "#F6C9E4"
        border_radius: 12
      }
      shadow: 'none'
    }
  
  viz {
    table {
      general {
        bg_color: '#FAE0EA';
        font_family: 'Playwrite GB S'
        banding_color: 'white'
        hover_color: '#F5CEDD'; 
        font_size: '12px'; 
        font_color: '#852C50'; 
        border_color: '#DA7AA1'; 
        grid_color: '#DA7AA1'; 
      }

      // Header Styles
      header {
        bg_color: '#F8A4C5';
        font_size: '14px'; 
        font_weight: 'bold'; 
      }

      // Row Header Styles
      row_header {
        bg_color: '#F8A4C5';
        font_size: '12px'; 
        font_weight: 'bold'; 
      }

      // Column Header Styles
      column_header {
        bg_color: 'linear-gradient(to top, #FFF0F6, white)';
        font_size: '12px'; 
        font_weight: 'normal'; 
      }

      // Subtitle Styles
      sub_title {
        font_size: '12px';
        font_weight: 'light';
      }
    }
  }
}



