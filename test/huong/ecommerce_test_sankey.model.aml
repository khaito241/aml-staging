Model ecommerce_test_sankey {
  type: 'query'
  label: 'Ecommerce Test Sankey'
  description: ''
  data_source_name: 'tuan_nguyen_db'
  dimension continent_name {
    label: 'Continent Name'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.continent_name }};;
  }
  dimension country_name {
    label: 'Country Name'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.country_name }};;
  }
  dimension city_name {
    label: 'City Name'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.city_name }};;
  }
  dimension country_city {
    label: 'Country City'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.country_city }};;
  }
  dimension order_counts {
    label: 'Order Counts'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.order_counts }};;
  }

  owner: 'huong.le@holistics.io'
  query: @sql select 
  c.continent_name
  , c.name as country_name 
  , ci.name as city_name 
  , concat(c.name, ' - ', ci.name) as country_city
  , count(o.id) as order_counts
  
from ecommerce.orders o 
left join ecommerce.order_items i on o.id = i.order_id
left join ecommerce.products p on i.product_id = p.id 
left join ecommerce.merchants m on p.merchant_id = m.id 
left join ecommerce.cities ci on m.city_id = ci.id 
left join ecommerce.countries c on ci.country_code = c.code
group by 1,2,3,4;;
  models: [
  ]
}
