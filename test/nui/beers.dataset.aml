Dataset beer {
  label: 'Beer'
  description: ''
  data_source_name: 'holisticsstaging'
  models: [
    beers,
  ]
  relationships: [
  ]
  owner: 'hoang.do@holistics.io'
  metric max_rating {
    label: "Max Rating"
    type: "number"
    hidden: false
    description: ""
    definition: @aql max(beers.rating);;
  }
  
  metric sum_ratings {
    label: "Sum Ratings"
    type: "number"
    hidden: false
    description: ""
    definition: @aql concat('a');;
  }
  
}