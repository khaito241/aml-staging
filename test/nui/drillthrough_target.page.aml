Dashboard drillthrough_target {
  title: 'Drillthrough Target'
  description: ''''''

  view: CanvasLayout {
    label: 'View 1'
    height: 840
    grid_size: 20
    block f1 {
      position: pos(20, 20, 300, 100)
      layer: 1
    }
    block f2 {
      position: pos(340, 20, 300, 100)
      layer: 2
    }
    block v1 {
      position: pos(20, 160, 400, 300)
      layer: 1
    }
  }

  theme: H.themes.classic
  block f1: FilterBlock {
    label: 'Order Time'
    type: 'date'
    source: FieldFilterSource {
      dataset: beer
      field: ref('beers', 'order_time')
    }
    default {
      operator: 'matches'
      value: '$H_NIL$'
    }
    settings {
      drillthrough: Drillthrough {
        enabled: true
        sources: [
          AutoDrillthroughSource {
          }
        ]
      }
    }
  }
  block f2: FilterBlock {
    label: 'Product'
    type: 'field'
    source: FieldFilterSource {
      dataset: beer
      field: ref('beers', 'product')
    }
    default {
      operator: 'is'
      value: []
    }
    settings {
      drillthrough: Drillthrough {
        enabled: true
        sources: [
          AutoDrillthroughSource {
          }
        ]
      }
    }
  }
  block v1: VizBlock {
    label: 'Test'
    viz: DataTable {
      dataset: beer
      calculation f_f2767ed {
        label: 'Sum of Revenue'
        formula: @aml beers.price * beers.i;;
        calc_type: 'dimension'
        data_type: 'number'
      }
      fields: [
        VizFieldFull {
          ref: ref('beers', 'product')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('beers', 'model')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('beers', 'gender')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('beers', 'price')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 5000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  interactions: [
    FilterInteraction {
      from: 'f1'
      to: [
        CustomMapping {
          block: 'f2'
          disabled: true
        }
      ]
    },
    FilterInteraction {
      from: 'f2'
      to: [
        CustomMapping {
          block: 'f1'
          disabled: true
        }
      ]
    }
  ]
}