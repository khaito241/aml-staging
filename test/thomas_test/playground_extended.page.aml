

Dashboard playground_extended = playground_dashboard.extend({
  view: layout_view.extend({
    block f1 { 
      position: pos(200, 100, 30, 30)
    }
  })

  block f1: FilterBlock {
    label: 'Monthly'
    type: 'date'
    default {
      operator: 'between'
      value: [
        '$H_NIL$',
        '$H_NIL$'
      ]
    }
  }
  interactions: [
    FilterInteraction {
      from: 'f1'
      to: [
        CustomMapping {
          block: 'v1'
          field: ref('users', 'sign_up_at')
        }
      ]
    }
  ]
})