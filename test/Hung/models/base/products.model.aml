Model products {
  type: 'table'
  label: "Products"
  description: ""
  owner: 'hung.nguyen@holistics.io'
  data_source_name: 'demodb'

  table_name: '\"ecommerce\".\"products\"'

  dimension id {
    label: 'Id'
    type: 'number'
  }

  dimension category_id {
    label: 'Category Id'
    type: 'number'
  }

  dimension name {
    label: 'Name'
    type: 'text'
  }

  dimension price {
    label: 'Price'
    type: 'number'
  }

  dimension created_at {
    label: 'Created At'
    type: 'date'
  }
  dimension order_date {
    label: "Order Date"
    type: "date"
    definition: @sql DATE({{ created_at }});;
    format: "LLL dd yyyy"
  }
}
