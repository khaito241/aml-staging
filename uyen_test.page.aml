Dashboard uyen_test {
  title: 'Uyen Test'
  description: ''''''
  view: CanvasLayout {
    label: 'View 1'
    block v1 {
      position: pos(0, 0, 400, 300)
    }
    block v2 {
      position: pos(480, 0, 400, 300)
    }
  }
  block v1: VizBlock {
    label: 'Sum of User Age by Gender'
    viz: PieChart {
      dataset: ecommerce
      legend: VizFieldFull {
        ref: ref('users', 'gender')
        format {
          type: 'text'
        }
      }
      series {
        field {
          ref: ref('users', 'age')
          aggregation: 'sum'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      }
      tooltips: [
        VizTooltip {
          field {
            ref: ref('users', 'age')
            aggregation: 'sum'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      ]
      settings {
        row_limit: 5000
      }
    }
  }
  block v2: VizBlock {
    label: 'format'
    viz: DataTable {
      dataset: ecommerce
      fields: [
        VizFieldFull {
          ref: ref('order_derived', 'order_created_at')
          format {
            type: 'date'
            pattern: 'dd LL yyyy'
            separator: '/'
          }
        },
        VizFieldFull {
          ref: ref('order_derived', 'delivery_attempts')
          format {
            type: 'number'
            pattern: '#,###0.00'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 10
      }
    }
  }
}