Model production_work_order {
  type: 'table'
  label: 'Work Order'
  description: ''
  data_source_name: 'adventure_works'
  dimension work_order_id {
    label: 'Work Order Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.work_order_id }};;
  }
  dimension product_id {
    label: 'Product Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.product_id }};;
  }
  dimension order_qty {
    label: 'Order Qty'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.order_qty }};;
  }
  dimension scrapped_qty {
    label: 'Scrapped Qty'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.scrapped_qty }};;
  }
  dimension scrapped_rate {
    label: 'Scrapped Rate'
    type: 'number'
    hidden: false
    definition: @aql production_work_order.scrapped_qty * 1.0 / production_work_order.order_qty ;;
  }
  dimension start_date {
    label: 'Start Date'
    type: 'datetime'
    hidden: false
    definition: @sql {{ #SOURCE.start_date }};;
  }
  dimension end_date {
    label: 'End Date'
    type: 'datetime'
    hidden: false
    definition: @sql {{ #SOURCE.end_date }};;
  }
  dimension due_date {
    label: 'Due Date'
    type: 'datetime'
    hidden: false
    definition: @sql {{ #SOURCE.due_date }};;
  }
  dimension scrap_reason_id {
    label: 'Scrap Reason Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.scrap_reason_id }};;
  }
  dimension modified_date {
    label: 'Modified Date'
    type: 'datetime'
    hidden: false
    definition: @sql {{ #SOURCE.modified_date }};;
  }

  owner: 'cuong.vc@holistics.io'
  table_name: '"production"."work_order"'
}
