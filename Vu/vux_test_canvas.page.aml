            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'city_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'full_name')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
        frozen_columns: 0
        column_styles: [
          ColumnStyle {
            key: 'orders_2_status'
            width: 113
          },
          ColumnStyle {
            key: 'users_1_first_name'
            width: 84
          },
          ColumnStyle {
            key: 'users_1_email'
            width: 62
          }
        ]
      }
    }
  }
