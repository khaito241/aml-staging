Dashboard vux_test_canvas {
  title: 'Vux Test Canvas'
  description: 'abc'

  block filter_block: FilterBlock {
    label: 'This is a Filter Block'
    type: 'text'
    default {
      operator: 'is'
      value: 'default'
    }
  }
  block v1: VizBlock {
    label: 'Count of Id by Month Created At and Status'
    viz: LineChart {
      dataset: mysql_vux_2
      calculation f_69ccd8f {
        label: 'Total filtered'
        formula: @aql count(orders_2.id) | of_all(orders_2.status, keep_filters: true);;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('orders_2', 'created_at')
        operator: 'matches'
        value: '2019 to 2022'
      }
      x_axis: VizFieldFull {
        ref: ref('orders_2', 'created_at')
        transformation: 'datetrunc year'
        format {
          type: 'date'
        }
      }
      legend: VizFieldFull {
        ref: ref('orders_2', 'status')
        format {
          type: 'text'
        }
      }
      y_axis {
        series {
          field {
            ref: ref('users_1', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      tooltips: [
        VizTooltip {
          field {
            ref: 'f_69ccd8f'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      ]
      settings {
        row_limit: 10000
      }
    }
  }
  block v2: VizBlock {
    label: 'test'
    viz: DataTable {
      dataset: mysql_vux_2
      fields: [
        VizFieldFull {
          ref: ref('orders_2', 'id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('orders_2', 'status')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'email')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'gender')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 10000
      }
    }
  }
  view: CanvasLayout {
    label: 'View 1'
    height: 910
    block filter_block {
      position: pos(40, 30, 400, 80)
    }
    block v1 {
      position: pos(40, 140, 830, 370)
    }
    block v2 {
      position: pos(40, 560, 390, 300)
    }
  }
  interactions: [
    FilterInteraction {
      from: 'v1'
      to: []
    },
    FilterInteraction {
      from: 'v2'
      to: []
    }
  ]

}