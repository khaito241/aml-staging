Dashboard vux_test_canvas {
  title: 'Vux Test Canvas'
  description: 'abc'
  block filter_block: FilterBlock {
    label: 'This is a Filter Block'
    type: 'field'
    source: FieldFilterSource {
      dataset: mysql_vux_23
      field: ref('orders_2', 'status')
    }
    default {
      operator: 'is'
      value: []
    }
  }
  block v1: VizBlock {
    label: 'Count of Id by Month Created At and Status'
    viz: LineChart {
      dataset: mysql_vux_23
      calculation f_69ccd8f {
        label: 'Total filtered'
        formula: @aql count(orders_2.id) | of_all(orders_2.status, keep_filters: true);;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('orders_2', 'created_at')
        operator: 'matches'
        value: '2019 to 2022'
      }
      x_axis: VizFieldFull {
        ref: ref('orders_2', 'created_at')
        transformation: 'datetrunc year'
        format {
          type: 'date'
        }
      }
      legend: VizFieldFull {
        ref: ref('orders_2', 'status')
        format {
          type: 'text'
        }
      }
      y_axis {
        series {
          field {
            ref: ref('users_1', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      tooltips: [
        VizTooltip {
          field {
            ref: 'f_69ccd8f'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      ]
      settings {
        row_limit: 10000
      }
    }
  }
  block v2: VizBlock {
    label: 'Table'
    viz: DataTable {
      dataset: mysql_vux_23
      fields: [
        VizFieldFull {
          ref: ref('orders_2', 'id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('orders_2', 'status')
          format {
            type: 'text'
          }
          uname: 'orders_2_status'
        },
        VizFieldFull {
          ref: ref('users_1', 'first_name')
          format {
            type: 'text'
          }
          uname: 'users_1_first_name'
        },
        VizFieldFull {
          ref: ref('users_1', 'last_name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('orders_2', 'delivery_attempts')
