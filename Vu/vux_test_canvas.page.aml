    label: 'Order count by status'
    viz: PivotTable {
      dataset: mysql_vux_23
      filter {
        field: ref('orders_2', 'created_at')
        operator: 'matches'
        value: '2020 to 2024'
      }
      rows: [
        VizFieldFull {
          ref: ref('orders_2', 'created_at')
