Dashboard vux_test_canvas {
  title: 'Vux Test Canvas'
  description: 'abc'
  block filter_block: FilterBlock {
    label: 'This is a Filter Block'
    type: 'field'
    source: FieldFilterSource {
      dataset: mysql_vux_2
      field: ref('orders_2', 'status')
    }
    default {
      operator: 'is'
      value: []
    }
  }
  block v1: VizBlock {
    label: 'Count of Id by Month Created At and Status'
    viz: LineChart {
      dataset: mysql_vux_2
      calculation f_69ccd8f {
        label: 'Total filtered'
        formula: @aql count(orders_2.id) | of_all(orders_2.status, keep_filters: true);;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('orders_2', 'created_at')
        operator: 'matches'
        value: '2019 to 2022'
      }
      x_axis: VizFieldFull {
        ref: ref('orders_2', 'created_at')
        transformation: 'datetrunc year'
        format {
          type: 'date'
        }
      }
      legend: VizFieldFull {
        ref: ref('orders_2', 'status')
        format {
          type: 'text'
        }
      }
      y_axis {
        series {
          field {
            ref: ref('users_1', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      tooltips: [
        VizTooltip {
          field {
            ref: 'f_69ccd8f'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      ]
      settings {
        row_limit: 10000
      }
    }
  }
  block v2: VizBlock {
    label: 'Table'
    viz: DataTable {
      dataset: mysql_vux_2
      fields: [
        VizFieldFull {
          ref: ref('orders_2', 'id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('orders_2', 'status')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'email')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'first_name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'last_name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('orders_2', 'delivery_attempts')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'gender')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'city_id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('users_1', 'full_name')
          format {
            type: 'text'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v3: VizBlock {
    label: 'Metric Sheet'
    viz: MetricSheet {
      dataset: mysql_vux_2
      date_field: VizFieldFull {
        ref: ref('orders_2', 'created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      rows: [
        MetricSeries {
          field: VizFieldFull {
            ref: ref('orders_2', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      ]
      settings {
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block d1: DateDrillBlock {
    label: 'Date Drill'
    default: 'year'
  }
  view: CanvasLayout {
    label: 'View 1'
    height: 1230
    block filter_block {
      position: pos(40, 30, 400, 86)
    }
    block v1 {
      position: pos(40, 490, 830, 370)
    }
    block v2 {
      position: pos(40, 130, 670, 300)
    }
    block v3 {
      position: pos(40, 870, 770, 190)
    }
    block d1 {
      position: pos(480, 30, 300, 80)
    }
  }
  interactions: [
    DateDrillInteraction {
      from: 'd1'
      to: [
        CustomMapping {
          block: 'v3'
          field: ref('orders_2', 'created_at')
        }
      ]
    }
  ]
}