Dashboard thomasto {
  title: 'Dashboard as-code in Modeling'
  description: 'fafafa haha afafaf '
  block bar_chart: VizBlock {
    label: 'A bar chart'
    viz: DataTable {
      dataset: ecommerce
      fields: [
        VizFieldFull {
          ref: ref('products', 'name')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('orders', 'id')
          aggregation: 'count'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
      }
    }
  }
  block bar_chart2: VizBlock {
    label: 'Another bar chart'
    viz: BarChart {
      dataset: ecommerce
      x_axis: VizFieldFull {
        ref: ref('products', 'name')
        format {
          type: 'text'
        }
      }
      y_axis {
        series {
          field {
            label: ''
            ref: ref('orders', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: 'auto'
          }
        }
      }
    }
  }
  block t2: TextBlock {
    content: @md # Dashboard 4.0 defined in Modeling;;
  }
  block v1: VizBlock {
    viz: ColumnChart {
      dataset: ecommerce
      filter {
        field: ref('order_items', 'quantity')
        operator: 'greater_than'
        value: '10'
      }
      x_axis: VizFieldFull {
        ref: ref('countries', 'name')
        format {
          type: 'text'
        }
      }
      y_axis {
        series {
          field {
            label: ''
            ref: ref('products', 'id')
            aggregation: 'count distinct'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: 'auto'
          }
        }
      }
    }
  }
  block f1: FilterBlock {
    label: 'number'
    type: 'number'
    default {
      operator: 'greater_than'
      value: '200'
    }
  }
  view: CanvasLayout {
    label: 'View 1'
    width: 1260
    height: 1170
    block bar_chart {
      position: pos(850, 180, 400, 300)
    }
    block t2 {
      position: pos(0, 0, 680, 90)
    }
    block v1 {
      position: pos(0, 140, 830, 340)
    }
    block f1 {
      position: pos(950, 90, 300, 80)
    }
    block bar_chart2 {
      position: pos(0, 500, 1250, 520)
    }
  }
}
