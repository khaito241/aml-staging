Dashboard unnamed_6 {
  title: 'Unnamed 6'
  description: ''''''

  view: CanvasLayout {
    label: 'View 1'
    height: 920
    block v1 {
      position: pos(10, 0, 1190, 430)
    }
    block v2 {
      position: pos(10, 430, 630, 460)
    }
    block v3 {
      position: pos(650, 440, 540, 450)
    }
  }
  block v1: VizBlock {
    label: 'All time monthly signups'
    viz: ColumnChart {
      dataset: ecommerce
      x_axis: VizFieldFull {
        ref: ref('users', 'sign_up_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      y_axis {
        series {
          field: VizFieldFull {
            ref: ref('users', 'email')
            aggregation: 'count distinct'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v2: VizBlock {
    label: 'Last 2 week daily GMV'
    viz: CombinationChart {
      dataset: ecommerce
      filter {
        field: ref('order_derived', 'order_created_at')
        operator: 'last'
        value: '2'
        modifier: 'week'
      }
      x_axis: VizFieldFull {
        ref: ref('order_derived', 'order_created_at')
        transformation: 'datetrunc day'
        format {
          type: 'date'
        }
      }
      y_axis {
        series {
          mark_type: 'area'
          field: VizFieldFull {
            ref: ref('order_derived', 'gross_merchandise_value')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 10000
        pop {
          field: ref('order_derived', 'order_created_at')
          duration: 1
          granularity: 'year'
          show_growth_rate: true
        }
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block v3: VizBlock {
    label: 'Recent 2 week GMV per categories'
    viz: BarChart {
      dataset: ecommerce
      filter {
        field: ref('order_derived', 'order_created_at')
        operator: 'last'
        value: '2'
        modifier: 'week'
      }
      filter {
        field: ref('categories', 'name')
        operator: 'not_null'
        value: []
      }
      x_axis: VizFieldFull {
        ref: ref('categories', 'name')
        format {
          type: 'text'
        }
      }
      y_axis {
        series {
          field: VizFieldFull {
            ref: ref('order_derived', 'gross_merchandise_value')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
          settings {
            color: '#D70909'
          }
        }
      }
      settings {
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
}