const layout_view = CanvasLayout {
    label: 'View 1'
    width: 1000
    height: 1000
    block t1 {
      position: pos(160, 30, 600, 100)
    }
    block v1 {
      position: pos(170, 160, 400, 300)
    }
  }

Dashboard playground_dashboard {
  title: 'Unnamed Dashboard'
  description: ''''''
  view: layout_view
  block t1: TextBlock {
    content: @md # Playground

This is very ephemeral, don't save anything here in hopes that you might need to seriously use it later.;;
  }
  block v1: VizBlock {
    label: 'Monthly Signup'
    viz: LineChart {
      dataset: ecommerce
      filter {
        field: ref('users', 'sign_up_at')
        operator: 'last'
        value: '1'
        modifier: 'year'
      }
      x_axis: VizFieldFull {
        ref: ref('users', 'sign_up_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      y_axis {
        series {
          field: VizFieldFull {
            ref: ref('users', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 10000
      }
    }
  }
}