Model aa_ecommerce_orders {
  type: 'table'
  label: 'Orders'
  description: ''
  data_source_name: 'demodw'
  dimension id {
    label: 'Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.id }};;
  }
  dimension status {
    label: 'Status'
    type: 'text'
    hidden: false
    definition: @sql {{ #SOURCE.status }};;
  }
  dimension created_at {
    label: 'Created At'
    type: 'datetime'
    hidden: false
    definition: @sql {{ #SOURCE.created_at }};;
  }
  dimension user_id {
    label: 'User Id'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.user_id }};;
  }
  dimension delivery_attempts {
    label: 'Delivery Attempts'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.delivery_attempts }};;
  }
  dimension discount {
    label: 'Discount'
    type: 'number'
    hidden: false
    definition: @sql {{ #SOURCE.discount }};;
  }
  dimension order_value {
    label: 'Order Value'
    type: 'number'
    hidden: false
    definition: @aql sum(aa_ecommerce_order_items.item_value);;
    format: "#,###[$$]"
  }
  dimension created_month {
    label: "Created Month"
    type: "date"
    definition: @sql DATE_TRUNC('month', {{ #SOURCE.created_at }});;
    description: "Transform created date to month"
    format: "LLL yyyy"
  }

  owner: 'scott.bui@holistics.io'
  table_name: '"ecommerce"."orders"'
}
