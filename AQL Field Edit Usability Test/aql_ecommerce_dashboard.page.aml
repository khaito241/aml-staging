use H {
  themes
}

Dashboard aql_field_dashboard {
  title: 'AQL Ecommerce Dashboard'
  description: ''''''
  block title: TextBlock {
    content: @md # Canvas Dashboard;;
  }
  block v2: VizBlock {
    label: 'running_total'
    viz: PivotTable {
      dataset: aql_field_edit_ut_dataset
      calculation untitled_metric_4010c11 {
        label: 'Untitled Metric'
        formula: @aql count(date_dim.date_d) | of_all(countries.name);;
        calc_type: 'measure'
        data_type: 'number'
      }
      filter {
        field: ref('date_dim', 'date_d')
        operator: 'matches'
        value: '2022'
      }
      rows: [
        VizFieldFull {
          ref: ref('date_dim', 'date_d')
          transformation: 'datetrunc week'
          format {
            type: 'date'
            pattern: 'wwww'
          }
        }
      ]
      columns: [
        VizFieldFull {
          ref: ref('countries', 'name')
          format {
            type: 'text'
          }
        }
      ]
      values: [
        VizFieldFull {
          ref: 'total_orders'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: 'running_total_order'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: 'untitled_metric_4010c11'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_with_no_data: true
        show_row_total: true
        show_column_total: true
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }

  view: CanvasLayout {
    label: 'View 1'
    height: 840
    grid_size: 20
    block title {
      position: pos(20, 20, 1160, 60)
    }
    block v2 {
      position: pos(20, 100, 1160, 500)
    }
  }

  theme: themes.classic
}