Dashboard dashboad_test_cua_bao {
  title: 'Dashboad Test Cua Bao'
  description: 'New description'

  block text_block: TextBlock {
    content: @md
      # Heading 1
      ### Heading 3
      This is a **dashboard** with _canvas layout_
    ;;
  }

  block text_filter: FilterBlock {
    type: 'text' // or 'field'
    label: 'Sample text filter'
    // use the below source for 'field' filter
    // source: FieldFilterSource {
    //   dataset: 'dataset'
    //   field: ref('data model', 'data field')
    // }
    default {
      operator: 'is'
      value: 'Default value'
    }
  }
  block v1: VizBlock {
    viz: DataTable {
      dataset: movies_analysis
      fields: [
        VizFieldFull {
          ref: ref('movies_metadata', 'id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: ref('movies_metadata', 'imdb_id')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('movies_metadata', 'original_title')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('movies_metadata', 'original_language')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('movies_metadata', 'tagline')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('movies_metadata', 'overview')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('movies_metadata', 'release_date')
          format {
            type: 'date'
          }
        }
      ]
      settings {
        show_row_number: true
      }
    }
  }
  block v2: VizBlock {
    label: 'as'
    viz: DataTable {
      dataset: rela_test
      calculation f_05fce4a {
        label: 'asdasd'
        formula: @aml ecommerce_analytics_user_cohort_retention.month_number + 1;;
        calc_type: 'dimension'
        data_type: 'number'
      }
      fields: [
        VizFieldFull {
          ref: ref('ecommerce_analytics_user_cohort_retention', 'cohort_size')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        },
        VizFieldFull {
          ref: 'f_05fce4a'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
      }
    }
  }
