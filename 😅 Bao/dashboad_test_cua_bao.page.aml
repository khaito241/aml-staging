  view: CanvasLayout {
    label: 'View 1'
    height: 800
    block text_block {
      position: pos(20, 60, 1200, 150)
    }
    block text_filter {
      position: pos(420, 310, 300, 100)
    }
    block v1 {
      position: pos(20, 280, 400, 300)
    }
    block v2 {
      position: pos(600, 430, 400, 300)
    }
  }
  settings {
    timezone: 'Asia/Singapore'
    autorun: false
    allow_timezone_change: true
    cache_duration: 10
  }

}