Dashboard test_custom_label_mail {
  title: 'Dish revenue'
  description: ''''''
  block v1: VizBlock {
    label: 'Overall'
    viz: DataTable {
      dataset: revenue
      fields: [
        VizFieldFull {
          ref: ref('zcommerce_dev_persisted_revenue', 'date')
          format {
            type: 'date'
          }
        },
        VizFieldFull {
          ref: ref('zcommerce_dev_persisted_revenue', 'order')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('zcommerce_dev_persisted_revenue', 'dishes')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          ref: ref('zcommerce_dev_persisted_revenue', 'type')
          format {
            type: 'text'
          }
        },
        VizFieldFull {
          label: 'Custom Price'
          ref: ref('zcommerce_dev_persisted_revenue', 'price')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 20
      }
    }
  }
  view: CanvasLayout {
    label: 'View 1'
    height: 800
    block v1 {
      position: pos(310, 40, 400, 300)
    }
  }

}