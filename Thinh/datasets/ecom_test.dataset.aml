Dataset ecommerce_test {
  label: 'Ecommerce'
  models: [ecommerce_users, ecommerce_products, ecommerce_orders, ecommerce_order_items, dim_dates]
  relationships: [
		relationship(ecommerce_orders.user_id > ecommerce_users.id, true),
		relationship(ecommerce_order_items.product_id > ecommerce_products.id, true),
		relationship(ecommerce_orders.created_date > dim_dates.date_key, true)
  ]
  data_source_name: 'demodb'

  view {
		model ecommerce_products {
			group product_info {
				field id
				field name
				field price
				field product_google_link
				field created_at
			}
			field category_id
			field merchant_id
		} // DISPLAY ALL FIELDS FROM MODEL "Products"

		model ecommerce_users {
      group user_info {
				field first_name
				field last_name
				field full_name
				field id
        field email
			  field age
				field gender
				field total_users
			}

			group sign_up_info {
				field sign_up_at
				field sign_up_date
				field users_signed_up_count
			}
		}

    // CREATE GROUP "Order Master" CONTAINING TWO MODELS "Orders" & "Dim dates"
		group order_master {
			model ecommerce_orders {
				field delivery_attempts
	      field status
				field discount
				group order_count {
					field total_orders_count
          field delivered_orders_count
					field cancelled_orders_count
					field refunded_orders_count
				}
	    }

			model ecommerce_order_items {
				field quantity
			}

	    model dim_dates {
	      field year
	      field month_name
	      // field day_of_month
	      field week_number
	      field quarter
	    }
		}
  }
}