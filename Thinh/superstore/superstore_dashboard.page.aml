  }
  block v9: VizBlock {
    label: 'Total Quantity Sold'
    viz: MetricKpi {
      dataset: superstore_dataset
      value: VizFieldFull {
        ref: ref('persisted_models_superstore', 'quantity')
        aggregation: 'sum'
      }
    }
    settings {
      hide_label: true
    }
  }
