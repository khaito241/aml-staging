Dashboard blank {
  title: 'Blank'
  description: ''''''
  view: CanvasLayout {
    label: 'View 1'
    height: 800
    block v1 {
      position: pos(200, 80, 560, 420)
    }
    block d1 {
      position: pos(780, 80, 300, 80)
    }
  }
  block v1: VizBlock {
    label: 'AOV by Month Order Created At'
    viz: AreaChart {
      dataset: ecommerce
      x_axis: VizFieldFull {
        ref: ref('order_derived', 'order_created_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      y_axis {
        series {
          field {
            ref: ref('order_derived', 'aov')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 5000
        legend_label: 'hidden'
      }
    }
    settings {
      hide_label: true
    }
  }
  block d1: DateDrillBlock {
    label: 'Date Drill'
  }
  interactions: [
    DateDrillInteraction {
      from: 'd1'
      to: [
        CustomMapping {
          block: 'v1'
          field: ref('order_derived', 'order_created_at')
        }
      ]
    }
  ]

}