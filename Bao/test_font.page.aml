}
.dac-body .cursor-pointer {
    font-family: Pangea, sans-serif;
    font-variation-settings: 'opsz' 10, 'wght' 500, 'ital' 0 !important;  
}
.dac-body ol, .dac-body ul {
    list-style: disc !important;
    margin: 15px 0 0 15px !important;
    text-align: left !important;
}
.dac-body ul li, .dac-body li {
    list-style-position: outside;
    font-size: 14px;
    font-variation-settings: 'opsz' 10, 'wght' 400, 'ital' 0 !important;
    line-height: 1.4;
    padding-left: 5px;
    margin-bottom: 5px;
    text-align: left !important;
}
.dw_italic {
    font-variation-settings: 'opsz' 10, 'wght' 500, 'ital' 1 !important;
}
.dw_bold {
    font-variation-settings: 'opsz' 10, 'wght' 600, 'ital' 0 !important;
}
.dac-body p {
  margin-bottom: -2px;
  margin-top: 0px;
  line-height: 21px;
  text-align: left !important;
}
.dac-body a, .dac-body a:link, .dac-body a:visited {
    color: #002186 !important;
    font-size: 15px;
    font-variation-settings: 'opsz' 10, 'wght' 600, 'ital' 0 !important;
}
.dac-body a:after {
    content: "";
    width: 22px;
    height: 22px;
    background:transparent url("http://dw.com/display/holistics/icons-indicator-ind-info.svg") no-repeat 0px 0px fixed;
    top: 6px;
    right: -5px;
    position: relative;
    display: inline-block;
    background-size: 18px 18px;
}
.dac-body a:hover {
    text-decoration: underline;
}
.dac-body a.no_arrow:after {
content: none;
}
.dac-body .single-value .html-value {
    line-height: 2.5 !important;
    margin: unset !important;
    text-align: left !important;
}
span.dac-viz-block-label {
    font-variation-settings: 'opsz' 20, 'wght' 500, 'ital' 0 !important;
    line-height: 110% !important;
    font-size: 23px;
    text-align: left !important;
}
span.dac-ic-block-label.truncate.font-medium {
    font-size: 18px !important;
    font-variation-settings: 'opsz' 20, 'wght' 500, 'ital' 0 !important;
    color: #000821 !important;
    text-align: top !important;
}
span.editable-span-display-value {
    font-size: 14px !important;
    color: #000821;
}
.dac-body .highlights {
text-align: left !important;
display: initial; 
margin: 0 !important; 
position: absolute; 
top: 0; 
left: 0;
}

.text-container.overflow {
border: 1px solid #99b5c9 / 0.0;
background-color: #f0f6fa /0.0;
    justify-content: start !important;
    padding-left: 10px;
    text-align: left !important;
}
.dm-filter-form.inline:not(.range_slider) {
border: 1px solid #99b5c9;
background-color: #f0f6fa;
    border-radius: 0px;
}
.text-container.overflow span {
color: #081336;
