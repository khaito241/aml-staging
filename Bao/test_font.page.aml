@import url('https://www.dw.com/display/fonts/pangea/100/var/DWPangeaLatinBasicLettersVAR.woff2');

.h-canvas.box-content.dac-canvas {
    background-image: url(https://www.dw.com/display/holistics/brand-dialogforms-parallelogram.svg);
    background-repeat: no-repeat;
    background-size: 56%;
    background-position: right bottom;
}

.dac-body p {
    font-family: Pangea, sans-serif;
    font-variation-settings: 'opsz' 10, 'wght' 400, 'ital' 0 !important;
    text-align: left !important;
 }
.dac-body h2 {
    color: #002186 !important; 
    font-family: Pangea, sans-serif;
