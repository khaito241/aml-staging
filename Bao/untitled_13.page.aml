@import url("https://www.dw.com/display/holistics/styles.css");
* {
  font-family: "Pangea" !important;
}
a,
a:link,
a:visited {
  color: #002186 !important;
  font-size: 15px;
  font-variation-settings: "opsz" 10, "wght" 600, "ital" 0 !important;
}
a:after {
  content: "";
  width: 22px;
  height: 22px;
  background: transparent
    url("http://dw.com/display/holistics/icons-indicator-ind-info.svg")
    no-repeat 0px 0px fixed;
  top: 6px;
  right: -5px;
  position: relative;
  display: inline-block;
  background-size: 18px 18px;
}
a:hover {
  text-decoration: underline;
}
a.no_arrow:after {
  content: none;
}
.single-value .text-container {
  justify-content: flex-start;
}
.dac-canvas {
  background-image: url(https://www.dw.com/display/holistics/brand-dialogforms-parallelogram.svg);
  background-repeat: no-repeat;
  background-size: 56%;
  background-position: right bottom;
}
.dac-viz-block:not(:has(.single-value-table)) .dac-viz-block-label {
  font-variation-settings: "opsz" 20, "wght" 500, "ital" 0 !important;
  line-height: 110% !important;
  font-size: 23px !important;
  text-align: left !important;
  color: #002186 !important;
}
.dw_italic {
  font-variation-settings: "opsz" 10, "wght" 500, "ital" 1 !important;
}
.dw_bold {
  font-variation-settings: "opsz" 10, "wght" 600, "ital" 0 !important;
}
.html-value ol,
.html-value ul {
  list-style: disc !important;
  margin: 15px 0 0 15px !important;
  text-align: left !important;
}
.html-value li {
  list-style-position: outside;
  font-size: 14px;
  font-variation-settings: "opsz" 10, "wght" 400, "ital" 0 !important;
  line-height: 1.4;
  padding-left: 5px;
  margin-bottom: 5px;
  text-align: left !important;
}
.html-value p {
  font-family: Pangea, sans-serif;
  font-variation-settings: "opsz" 10, "wght" 400, "ital" 0 !important;
  text-align: left !important;
}
.html-value h2 {
  color: #002186 !important;
  font-family: Pangea, sans-serif;
  font-variation-settings: "opsz" 20, "wght" 600, "ital" 0 !important;
  font-size: 27px !important;
  text-align: left !important;
}

.html-value h3,
.html-value h4 {
  color: #002186 !important;
  font-family: Pangea, sans-serif;
  font-variation-settings: "opsz" 20, "wght" 500, "ital" 0 !important;
  font-size: 23px !important;
  text-align: left;
  text-align: left !important;
}
  ;;
}

