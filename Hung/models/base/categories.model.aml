Model categories {
  type: 'table'
  label: "Categories2"
  description: ""
  owner: 'hung.nguyen@holistics.io'
  data_source_name: "phuong_db"

  table_name: '\"ecommerce\".\"categories\"'
  dimension id {
    label: "Id"
    type: "number"
    definition: @sql {{ #SOURCE.id }};;
  }

  dimension name {
    label: 'Name'
    type: 'text'
  }
  measure total_categories {
    label: "Total Categories"
    type: "number"
    definition: @sql count({{ id }});;
    aggregation_type: "custom"
  }
}
