import "./orders.model.aml" { orders }

Model cancelled_orders {
  type: 'query'
  label: "Orders"
  data_source_name: 'demodb'
  owner: 'khai@holistics.io'

  // Dependent models
  models: [orders]
  query: @sql 
    SELECT {{ #orders.* }} FROM {{ #orders }}
    WHERE {{ #orders.status }} = 'cancelled'
  ;;


  dimension id {
    label: 'Id'
    type: 'number'
  }

  dimension status {
    label: 'Status'
    type: 'text'
  }

  dimension created_at {
    label: 'Created At'
    type: 'date'
  }
}