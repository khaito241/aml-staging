Dashboard ecommerce_dashboard {
  title: 'Ecommerce Dashboard'
  description: 'Thomas 1 dashboard for demo purpose'
  block text_block: TextBlock {
    content: @md  # User registration
- This dashboard helps you explore user registration.
;;
  }
  block text_filter: FilterBlock {
    label: 'Time span'
    type: 'date'
    default {
      operator: 'matches'
      value: '2023-11-01 - 2023-12-31'
    }
  }
  block v1: VizBlock {
    label: 'Weekly registration'
    description: 'Registration on a weekly basis (to be refactored into just registration metric)'
    viz: PivotTable {
      dataset: ecommerce
      rows: [
        VizFieldFull {
          ref: ref('users', 'sign_up_at')
          transformation: 'datetrunc year'
          format {
            type: 'date'
            pattern: 'yyyy'
          }
        }
      ]
      values: [
        VizFieldFull {
          ref: ref('users', 'id')
          aggregation: 'count'
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_total: true
        show_column_total: true
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block t1: TextBlock {
    content: @md <div style="color:red;font-size:40px;background-color:yellow">Super awesome Ecommerce dashboard</div>

*Mission*: Just take over Shopee as the top Ecommerce player in Vietnam.
;;
  }
