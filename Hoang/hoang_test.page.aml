Dashboard hoang_test {
  title: 'Hoang Test'
  description: ''''''
  view: TabLayout {
    label: 'View 1'
    tab tab1: CanvasLayout {
      label: 'Tab 1'
      height: 800
      block v1 {
        position: pos(20, 100, 600, 300)
      }
      block d1 {
        position: pos(20, 10, 600, 80)
      }
      block d2 {
        position: pos(20, 470, 600, 80)
      }
      block v2 {
        position: pos(640, 20, 530, 300)
      }
    }
    tab tab2: CanvasLayout {
      label: 'Tab 2'
      height: 800
      grid_size: 20
      mobile {
        mode: 'auto'
      }
      block t1 {
        position: pos(20, 20, 100, 60)
        layer: 1
      }
      block t2 {
        position: pos(140, 20, 100, 100)
        layer: 2
      }
      block v3 {
        position: pos(260, 20, 400, 300)
        layer: 3
      }
      block f1 {
        position: pos(720, 40, 300, 100)
        layer: 4
      }
    }
  }
  block v1: VizBlock {
    label: 'Test'
    viz: AreaChart {
      dataset: aql_ecommerce
      theme {

      }
      x_axis: VizFieldFull {
        ref: ref('users', 'sign_up_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      legend: VizFieldFull {
        ref: ref('users', 'gender')
        format {
          type: 'text'
        }
      }
      y_axis {
        settings {
          reverse_stack: true
          stack_series_by: 'value'
        }
        series {
          field: VizFieldFull {
            ref: ref('users', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
            break_by_legend: false
          }
        }
        series {
          field: VizFieldFull {
            ref: ref('users', 'total_users')
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 5000
        x_axis_show_null_datetime: true
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block d1: DateDrillBlock {
    label: 'DD'
  }
  block d2: DateDrillBlock {
    label: 'DD (dup)'
  }
  block v2: VizBlock {
    label: 'Test'
    viz: LineChart {
      dataset: aql_ecommerce
      x_axis: VizFieldFull {
        ref: ref('users', 'sign_up_at')
        transformation: 'datetrunc month'
        format {
          type: 'date'
        }
      }
      y_axis {
        series {
          field: VizFieldFull {
            ref: ref('users', 'id')
            aggregation: 'count'
            format {
              type: 'number'
              pattern: 'inherited'
            }
          }
        }
      }
      settings {
        row_limit: 5000
      }
    }
  }
  block t1: TextBlock {
    content: @md hihihi;;
  }
  block t2: star()
  block v3: VizBlock {
    label: 'test'
    viz: DataTable {
      dataset: aql_ecommerce
      theme {

      }
      fields: [
        VizFieldFull {
          ref: ref('users', 'id')
          format {
            type: 'number'
            pattern: 'inherited'
          }
        }
      ]
      settings {
        show_row_number: true
        row_limit: 10000
        aggregate_awareness {
          enabled: true
          debug_comments: true
        }
      }
    }
  }
  block f1: FilterBlock {
    label: 'Date'
    type: 'date'
    default {
      operator: 'matches'
      value: '$H_NIL$'
    }
  }
  interactions: [
    FilterInteraction {
      from: 'v1'
      to: [
        CustomMapping {
          block: 'v3'
          disabled: true
        }
      ]
    },
    FilterInteraction {
      from: 'v2'
      to: [
        CustomMapping {
          block: 'v3'
          disabled: true
        }
      ]
    },
    FilterInteraction {
      from: 'v3'
      to: [
        CustomMapping {
          block: [
            'v1',
            'v2'
          ]
          disabled: true
        }
      ]
    },
    DateDrillInteraction {
      from: 'd1'
      to: [
        CustomMapping {
          block: [
            'v1',
            'v2'
          ]
          field: ref('users', 'sign_up_at')
        }
      ]
    }
  ]
}